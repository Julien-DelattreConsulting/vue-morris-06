    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --colors --config build/webpack.build.conf.js"
  },
  "dependencies": {
    "babel-runtime": "^6.0.0",
    "morris.js": "github:pierresh/morris.js#master",
