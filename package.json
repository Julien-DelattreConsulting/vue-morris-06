{
  "name": "vue-morris-06",
  "description": "Vue.js component wrapping Morris.js 06 lib",
  "version": "0.1.0",
  "author": "Julien Delattre <julien.delattre@delattreconsulting.com>",
  "private": false,
  "keywords": [
    "vue",
    "vuejs",
    "morris.js",
    "morris.js 06"
  ],
  "main": "dist/vue-morris.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Julien-DelattreConsulting/vue-morris-06.git"
  },
  "bugs": "https://github.com/Julien-DelattreConsulting/vue-morris-06/issues",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --content-base ./examples --open --inline --hot --colors --config build/webpack.dev.conf.js",
